<% include templates/header.ejs %>
  <a class="menuoptions" href="/committee/<%= id %>/<%= club_id %>/teams/<%= team_id %>/addteammembers">Add Team Members</a> 
  <a class="menuoptions" href="/committee/<%= id %>/<%= club_id %>/teams/<%= team_id %>/deleteteammembers">Remove Team Members</a> 
    <a class="menuoptions" href="/committee/<%= id %>/<%= club_id %>/teams/<%= team_id %>/deleteteam">Delete Team</a> 
  <table class="liftlist largemenu memberlayout">
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
    </tr>
     <% for(var i=0; i < members_list.length; i++) {%>
    <tr class="member">
      <td class="member"><%= members_list[i].first_name %></td>
      <td class="member"><%= members_list[i].last_name %></td>
      <td class="member"><%= members_list[i].email %></td>
    </tr>
       <% } %>
  </table>
</body>
</html>

